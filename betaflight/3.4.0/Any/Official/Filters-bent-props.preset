#preflight-preset-v0.1
#AUTHOR: ctzsnooze
#DESCRIPTION: A typical quad on a decent frame that needs to tolerate bent props
#REQUIREMENTS_START
#FIRMWARE: betaflight
#MAIN_VERSION: 3.4
#SUB_VERSION: ANY
#REQUIREMENTS_END
set gyro_lowpass_type = PT1
set gyro_lowpass_hz = 120
set gyro_lowpass2_type = PT1
set gyro_lowpass2_hz = 300
set dterm_lowpass_hz = 80
set dterm_lowpass2_hz = 160 
