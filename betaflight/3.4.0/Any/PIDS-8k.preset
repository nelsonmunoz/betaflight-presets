#preflight-preset-v0.1
#AUTHOR: UAVtech
#DESCRIPTION:
#REQUIREMENTS_START
#FIRMWARE: betaflight
#MAIN_VERSION: 3.4
#SUB_VERSION: ANY
#REQUIREMENTS_END

set rc_interp_ch = RPYT
set rc_smoothing_type = FILTER
set rc_smoothing_input_hz = 25
set rc_smoothing_derivative_hz = 25


set setpoint_relax_ratio = 10
set dterm_setpoint_weight = 100

set iterm_relax = RP

set throttle_boost = 50
set throttle_boost_cutoff = 40

set p_roll = 46
set i_roll = 45
set d_roll = 25

set p_pitch = 50
set i_pitch = 50
set d_pitch = 27

set p_yaw = 65
set i_yaw = 45

set tpa_rate = 15
set tpa_breakpoint = 1050

set throttle_limit_type = CLIP
set throttle_limit_percent = 95
